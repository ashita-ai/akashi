# Postgres password (used by both postgres container and connection strings)
POSTGRES_PASSWORD=kyoyu

# Admin bootstrap API key (change for production)
KYOYU_ADMIN_API_KEY=changeme

# OpenAI API key for embeddings (optional; falls back to noop provider)
OPENAI_API_KEY=

# Embedding model (default: text-embedding-3-small)
KYOYU_EMBEDDING_MODEL=text-embedding-3-small

# Log level: debug, info, warn, error
KYOYU_LOG_LEVEL=info
